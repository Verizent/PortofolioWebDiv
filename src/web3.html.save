    <iframe class="lg:hidden md:block py-0 px-auto text-center mx-auto items-center" width="600" height="342" src="https://www.youtube.com/embed/10QQTDNigc"></iframe>

    <!--Hero-->
    <div class="w-full h-auto py-3 mx-auto">
    </br>
    </br>
    </br>
      <a href="#" class="bg-white text-black hover:text-slate-500 text-center font-semibold text-3xl">
